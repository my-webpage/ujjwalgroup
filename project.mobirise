br-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row {{contentAlign}}\">\n            <div class=\"col-12 col-lg-{{formWidth}}\">\n                <div class=\"mbr-section-head\" mbr-if=\"showTitle || showSubtitle\">\n                    <h3 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                        <b>GET IN <a href=\"#\" class=\"text-white\">TOUCH</a></b>\n                    </h3>\n                    <h4 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                        Sed risus pretium quam vulputate dignissim\n                    </h4>\n                </div>\n\n                <div class=\"form-wrap\">\n                    <div mbr-form class=\"mbr-form\">\n<!--Formbuilder Form-->\n<form action=\"adv.niranjanpawar@gmail.com\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler mx-auto\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for filling out the form!</div>\n<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\"> Oops...! some problem! </div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-12 form-group mb-3 mb-3\" data-for=\"name\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"name\" class=\"form-control-label mbr-fonts-style\"> Name </label>\n<input type=\"text\" name=\"name\" placeholder=\"name\" data-form-field=\"name\" mbr-theme-style=\"display-7\" class=\"form-control display-7\" value>\n</div>\n<div class=\"col-12 form-group mb-3 mb-3\" data-for=\"email\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"email\" class=\"form-control-label mbr-fonts-style\"> Email </label>\n<input type=\"email\" name=\"email\" placeholder=\"email\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control display-7\" value>\n</div>\n<div data-for=\"Mobile Number\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Mobile Number\" class=\"form-control-label mbr-fonts-style\"> Subject </label>\n<input type=\"text\" name=\"Mobile Number\" placeholder=\"Mobile Number\" data-form-field=\"Mobile Number\" mbr-theme-style=\"display-7\" class=\"form-control display-7\" value>\n</div>\n<div data-for=\"message\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"message\" class=\"form-control-label mbr-fonts-style\"> Message </label>\n<textarea name=\"message\" data-form-field=\"message\" mbr-theme-style=\"display-7\" class=\"form-control display-7\" value=\" \"> </textarea>\n</div>\n<div class=\"col-auto mbr-section-btn align-center\" mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\">\n<a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\" class=\"w-100 w-100 btn btn-success\">Send</a>\n</div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n                </div>\n\n\n<!--                <div class=\"col-auto mbr-section-btn align-center\" mbr-buttons=\"true\"-->\n<!--                     mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\">-->\n<!--                    <a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\"-->\n<!--                       class=\"btn btn-primary\">Send</a>-->\n<!--                </div>-->\n\n\n            </div>\n        </div>\n    </div>\n</section>",
