          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider1\" group=\"Gallery &amp; Slider\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <input type=\"id\" name=\"sliderId\">\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverse\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Details\" name=\"showDetails\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line\" name=\"showLine\" condition=\"showDetails\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"lineColor\" value=\"#ffffff\" condition=\"showDetails &amp;&amp; showLine\">\n        <input type=\"range\" title=\"Opacity\" name=\"lineOpacity\" min=\"0.1\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showDetails &amp;&amp; showLine\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <select title=\"Align\" name=\"alignButtons\" condition=\"showButtons\">\n            <option value=\"justify-content-start\" selected=\"\">Left</option>\n            <option value=\"justify-content-center\">Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Indicators\" name=\"showIndicators\">\n        <input type=\"color\" title=\"Color\" name=\"indicatorsColor\" value=\"#ffffff\" condition=\"showIndicators\">\n        <input type=\"checkbox\" title=\"Controls\" name=\"showControls\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"controlsColor\" value=\"#1f1437\" condition=\"showControls\">\n        <input type=\"color\" title=\"Hover\" name=\"controlsHoverColor\" value=\"#33d5ad\" condition=\"showControls\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"3\" max=\"10\" value=\"5\" step=\"1\" condition=\"autoplay\">\n        <header>Nutrition</header>\n        <input type=\"checkbox\" title=\"Show\" name=\"showNutrition\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" condition=\"showNutrition\" checked=\"\">\n        <input type=\"checkbox\" title=\"Decoration\" name=\"decoration\" condition=\"showNutrition\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"decorationColor\" value=\"#1f1437\" condition=\"showNutrition &amp;&amp; decoration\">\n        <header condition=\"showNutrition\">Card Background</header>\n        <input type=\"checkbox\" title=\"1 Transparent\" name=\"cardOneTr\" condition=\"showNutrition\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardOneBg\" value=\"#1f1437\" condition=\"showNutrition &amp;&amp; !cardOneTr\">\n        <input type=\"checkbox\" title=\"2 Transparent\" name=\"cardTwoTr\" condition=\"showNutrition\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardTwoBg\" value=\"#1f1437\" condition=\"showNutrition &amp;&amp; !cardTwoTr\">\n        <input type=\"checkbox\" title=\"3 Transparent\" name=\"cardThreeTr\" condition=\"showNutrition\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardThreeBg\" value=\"#1f1437\" condition=\"showNutrition &amp;&amp; !cardThree